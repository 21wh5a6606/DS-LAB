#include<stdio.h>
int linear_search(int n,int a[n] ,int x)
      {
          int i;
          for (i= 0;i <n; i++)
          {
              if(a[i] == x)
                return i;
        }
        return -1;
    }
    int main()
    {
        int a[10],n,i,x,pos;
            printf(" enter the list of elements\n");
           scanf("%d",&n);
                for(int i= 0;i<n;i++)
                {
                 scanf("%d",& a[i]);}
                 printf("enter the key value\n");
                 scanf("%d",&x);
  
        pos = linear_search(n,a,x);
            if(pos == -1)
                printf("element not found");
            else
                printf("element found at %d", pos);
   }
 
